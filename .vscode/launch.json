    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceFolder}",
            "executable": "./build/stm_basic_cmake.elf",
            "name": "Debug F1 (J-Link)",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32F103CB",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "rtos": "FreeRTOS",
            "svdFile" : "${workspaceFolder}/system/svd/STM32F103xx.svd"
        },
        {
            "cwd": "${workspaceFolder}",
            "executable": "./build/stm_basic_cmake.elf",
            "name": "Debug F4 (J-Link)",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32F407VG",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "rtos": "FreeRTOS",
            "svdFile" : "${workspaceFolder}/system/svd/STM32F407.svd"
        },
        {
            "cwd": "${workspaceFolder}",
            "executable": "./build/stm_basic_cmake.elf",
            "name": "Debug F0 (J-Link)",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32F030C8",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "rtos": "FreeRTOS",
            "svdFile" : "${workspaceFolder}/system/svd/STM32F030.svd"
        }
